{
  "name":"rubybookshelf",
  "scripts":{
    "postdeploy":"bin/setup"
  },
  "env":{
    "APPLICATION_HOST":{
      "required":true
    },
    "ASSET_HOST":{
      "required":true
    },
    "EXECJS_RUNTIME":{
      "required":true
    },
    "LANG":{
      "required":true
    },
    "RACK_ENV":{
      "required":true
    },
    "RAILS_ENV":{
      "required":true
    },
    "RAILS_SERVE_STATIC_FILES":{
      "required":true
    },
    "SECRET_KEY_BASE":{
      "required":true
    },
    "SMTP_ADDRESS":{
      "required":true
    },
    "SMTP_DOMAIN":{
      "required":true
    },
    "SMTP_PASSWORD":{
      "required":true
    },
    "SMTP_USERNAME":{
      "required":true
    }
  },
  "addons":[
    "heroku-postgresql"
  ]
}
